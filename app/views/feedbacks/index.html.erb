<p id="notice"><%= notice %></p> 

<h1>Feedback</h1>

<%= link_to 'Home', root_path %>

<table>
  <thead>
    <tr>
      <th>Event ID</th> 
      <th>Event Name</th>  
      <th>Event Review</th>  
      <th>Event Score</th>  
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback.each do |feedback| %>
      <tr>
        <td><%= feedback.event_id %></td>  
        <td><%= Event.find_by!(:id => feedback.event_id).title %></td>  
        <td><%= feedback.event_review %></td>
        <td><%= feedback.event_rating_score %></td>
        <td><%= link_to 'Show', feedback %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'View all Feedback', feedbacks_path %>